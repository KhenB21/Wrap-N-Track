{
  "expo": {
    "name": "wrapntrack",
    "slug": "wrapntrack",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Pensee logos/pensee-logo-only.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/Pensee logos/pensee-logo-only.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Pensee logos/pensee-logo-only.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/Pensee logos/pensee-logo-only.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "47966223-e765-47e7-ba09-e04f6c4e0e17"
      }
    }
  }
}
