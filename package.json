{
  "name": "wrap-n-track",
  "version": "1.0.0",
  "description": "Wrap-N-Track application with server and client",
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "preinstall": "cd Website/server && npm rebuild bcrypt --build-from-source",
    "start": "cd Website/server && npm start",
    "server": "cd Website/server && npm run dev",
    "client": "cd Website/client && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "wrap-n-track": "file:"
  }
}
