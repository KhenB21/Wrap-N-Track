    "express": "^5.1.0",
    "nodemailer": "^7.0.3",
    "pg": "^8.16.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-toastify": "^11.0.5",
    "uuid": "^11.1.0",
    "wrap-n-track": "file:"
  }
}
